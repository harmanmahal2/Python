current_users = ['harman', 'jatt', 'fuddu', 'ronaldo', 'rooney']

new_users = ['jatt', 'hehe', 'rammie', 'harman', 'kevin']
case = []

for each in current_users:
    lower = each.lower()
    case.append(lower)

for every in new_users:
    if every.lower() in case:
        print("username already exists")
    else:
        print('username is available')